{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api-proxy/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/service-worker.js",
      "destination": "/api/index.js"
    },
    {
      "source": "/public/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api|_next|_static|favicon.ico).*)",
      "destination": "/api/index.js"
    }
  ]
}
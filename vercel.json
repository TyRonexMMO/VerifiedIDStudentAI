{
  "version": 2,
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/api-proxy/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/service-worker.js",
      "destination": "/api/index.js"
    },
    {
      "source": "/public/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/((?!api|_next|_static|favicon.ico).*)",
      "destination": "/api/index.js"
    }
  ]
}